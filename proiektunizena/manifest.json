{
  "manifest_version": 2,
  "name": "CaptureExtension",
  "version": "1.0",
  "permissions": ["storage", "activeTab"],
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Capture Extension",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png",
    "96": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}

  